<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".uploadimg_activity">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@drawable/background"
            android:scaleType="centerCrop"
            android:gravity="center_horizontal">
            <TextView
                android:id="@+id/banner_logi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:text="@string/Your_Profile"
                android:textAppearance="@style/banner_logi"
                android:textSize="32sp"/>
            <RelativeLayout
                android:id="@+id/frame_25"
                android:layout_width="374dp"
                android:layout_height="693dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/frame_25">
                <ImageView
                    android:id="@+id/arrow_back"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="331dp"
                    android:src="@drawable/arrow_back"
                    android:layout_marginTop="15dp"/>
                <TextView
                    android:id="@+id/subtitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/subtitle"
                    android:textAppearance="@style/subtitle"
                    android:lineSpacingExtra="1sp"
                    android:layout_marginTop="15dp"
                    android:textSize="16sp"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentTop="true"/>

                <ImageView
                    android:id="@+id/user_avatar"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_below="@id/subtitle"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/avatar2" />
                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/title"
                    android:textAppearance="@style/title"
                    android:textSize="29sp"
                    android:layout_below="@id/user_avatar"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:id="@+id/update_prof"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/title"
                    android:layout_marginTop="25dp"
                    android:text="@string/update_prof"
                    android:textAppearance="@style/update_prof"
                    android:lineSpacingExtra="-13sp"
                    android:textSize="29sp"
                    />
                <View
                    android:id="@+id/line_1"
                    android:layout_width="289dp"
                    android:layout_height="1dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/line_3"
                    android:layout_below="@id/update_prof"
                    />
                <RelativeLayout
                    android:id="@+id/rectangle_btn_blue"
                    android:layout_width="259dp"
                    android:layout_height="66dp"
                    android:layout_below="@id/line_1"
                    android:layout_marginTop="38.5dp"
                    android:layout_centerHorizontal="true"

                    android:background="@drawable/rectangle_btn_blue">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/mobile_friendly"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:id="@+id/imgmobile"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@id/imgmobile"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:text="@string/choose_from"
                        android:textSize="18sp"
                        android:textAppearance="@style/choose_from"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rectangle_btn_pink"
                    android:layout_width="259dp"
                    android:layout_height="66dp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/rectangle_btn_blue"
                    android:layout_marginTop="38dp"
                    android:background="@drawable/rectangle_btn_pink">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/pinkcamera"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:id="@+id/pinkcamera"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@id/pinkcamera"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:text="@string/take_pictur"
                        android:textSize="18sp"
                        android:textAppearance="@style/take_pictur"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rectangle_btn_green"
                    android:layout_width="259dp"
                    android:layout_height="66dp"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/rectangle_btn_pink"
                    android:layout_marginTop="38dp"
                    android:background="@drawable/rectangle_btn_green">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/signalcheck"
                        android:layout_centerVertical="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:id="@+id/signalcheck"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@id/signalcheck"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:text="@string/confirmbtn"
                        android:textSize="18sp"
                        android:textAppearance="@style/confirmbtn"
                        />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/frame_26"
                android:layout_width="326dp"
                android:layout_height="47dp"
                android:layout_marginTop="20dp">
                <RelativeLayout
                    android:id="@+id/tab_bar"
                    android:layout_width="306dp"
                    android:layout_height="43dp"
                    android:background="@drawable/tab_bar"
                    android:layout_centerHorizontal="true"
                    android:clipToOutline="true">

                    <RelativeLayout
                        android:id="@+id/iconhome"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="13dp"
                        android:background="@drawable/home"
                        />

                    <RelativeLayout
                        android:id="@+id/iconsetting"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="13dp"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/setting"
                        />

                    <RelativeLayout
                        android:id="@+id/iconlocation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="13dp"
                        android:background="@drawable/location"
                        />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>